{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@now/static-build",
      "config": {
        "outputDirectory": "."
      }
    }
  ],
  "routes": [
    {
      "src": "/",
      "dest": "/index.html"
    },
    {
      "src": "/ducky",
      "dest": "https://ddg.gg"
    }
  ]
}