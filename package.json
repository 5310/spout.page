{
  "name": "spout.page",
  "private": true,
  "version": "0.0.0",
  "description": "the Spout website",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/5310/spout.page.git"
  },
  "author": "Sayantan Chaudhuri",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/5310/spout.page/issues"
  },
  "homepage": "https://github.com/5310/spout.page#readme",
  "main": "index.js",
  "scripts": {
    "deploy": "echo 'run `$ now --prod`'",
    "preview": "now",
    "dev": "npm run build && npx concurrently 'npm run watch' 'npm run serve'",
    "lint": "npm run lint:script",
    "test": "npm run test:script",
    "build": "npm run test && npm run build:script && npm run build:style",
    "watch": "npx concurrently 'npm run watch:script' 'npm run watch:style'",
    "serve": "npx concurrently 'npx serve src -l 5000' 'npx lt -p 5000'",
    "watch:script": "npx tsc -w",
    "watch:style": "npx postcss src/**/*.pcss --base src --dir src --ext css --watch",
    "lint:script": "npx tslint -p .",
    "lint:style": "npx stylelint src/**/*.pcss",
    "test:script": "npx jest --passWithNoTests",
    "build:script": "npx tsc",
    "build:style": "npx postcss src/**/*.pcss --base src --dir src --ext css",
    "postinstall": "npx snowpack"
  },
  "devDependencies": {
    "@types/jest": "^25.2.1",
    "concurrently": "^5.1.0",
    "jest": "^25.4.0",
    "localtunnel": "^2.0.0",
    "postcss-arithmetic": "0.0.4",
    "postcss-cli": "^7.1.0",
    "postcss-font-magician": "^2.3.1",
    "postcss-implicit-var": "0.0.1",
    "postcss-insert": "^1.0.0",
    "postcss-nested": "^4.2.1",
    "serve": "^11.3.0",
    "snowpack": "^1.7.0",
    "stylelint": "^13.3.2",
    "stylelint-config-standard": "^20.0.0",
    "ts-jest": "^25.4.0",
    "typescript": "^3.8.3",
    "typescript-tslint-plugin": "^0.5.5"
  }
}